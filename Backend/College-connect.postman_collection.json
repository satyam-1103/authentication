					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"var template = `",
									"<div>",
									"    <p>Success: {{response.success}}</p>",
									"    <p>Message: {{response.message}}</p>",
									"</div>",
									"`;",
									"",
									"function constructVisualizerPayload() {",
									"    return {response: pm.response.json()};",
									"}",
									"",
									"pm.visualizer.set(template, constructVisualizerPayload());"
								],
								"type": "text/javascript",
								"packages": {}
							}
						},
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"function getEventCategory(){\r",
									"    const category = [\"CULTURAL\", \"TECHNICHAL\"]\r",
									"    return category[Math.floor(Math.random() * category.length)]\r",
									"}\r",
									"\r",
									"const randomCat = getEventCategory()\r",
									"\r",
									"pm.environment.set(\"randomCategory\", randomCat)\r",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
